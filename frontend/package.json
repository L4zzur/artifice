    "generate:api": "openapi-ts"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.87.5",
    "@sveltejs/adapter-node": "^5.4.0",
    "@sveltejs/kit": "^2.48.5",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "svelte": "^5.41.0",
    "svelte-check": "^4.3.3",
    "typescript": "^5.9.3",
    "vite": "^7.1.10"
  },
  "dependencies": {
    "lucide-svelte": "^0.553.0"
  },
  "pnpm": {
    "overrides": {
      "cookie": "0.7.0"
    }
  }
}
